    }

    newsSelectedList {
      iconIdentifier = ext-news-wizard-icon
      title = LLL:EXT:news/Resources/Private/Language/locallang_be.xlf:flexforms_general.mode.news_selectedlist
      description = Liste an bestimmten news
      tt_content_defValues {
        CType = list
        list_type = news_selectedlist
      }
    }

    tagList {
      iconIdentifier = ext-news-wizard-icon
